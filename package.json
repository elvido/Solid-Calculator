{
  "name": "solid-calculator",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "build:dev": "rollup -w -c rollup.config.dev.mjs",
    "build:prd": "rollup -c rollup.config.prd.mjs",
    "start:mock": "cross-env MOCK=development nodemon --config mock-server/nodemon.json",
    "start:frontend": "cross-env MOCK=development yarn build:dev",
    "start:dev": "concurrently -n frontend,mock -c blue,green \"yarn start:frontend\" \"yarn start:mock\"",
    "start:preview": "node ./plugins/expressServe.mjs dist",
    "build:preview": "yarn build:dev && yarn start:preview",
    "format:update": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-eslint": "^9.1.0",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@solidjs/router": "^0.15.3",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/minimatch": "^6.0.0",
    "@types/web": "^0.0.281",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.21",
    "babel-preset-solid": "^1.9.9",
    "concurrency": "^0.1.4",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "daisyui": "^5.3.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-solid": "^0.14.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "http-proxy-node16": "^1.0.6",
    "mime-types": "^3.0.1",
    "minimist": "^1.2.8",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "open": "^10.2.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^4.0.1",
    "postcss-minify": "^1.2.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-preset-solid": "^3.0.0",
    "solid-js": "^1.9.9",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "url": "^0.11.4"
  },
  "resolutions": {
    "http-proxy": "npm:http-proxy-node16@^1.0.6",
    "glob": "^9.0.0",
    "stable": "^0.1.8"
  },
  "packageManager": "yarn@4.10.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elvido/Solid-Calculator.git"
  },
  "keywords": [
    "SolidJS",
