{
  "name": "solid-calculator",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "rollup -w -c rollup.config.dev.js",
    "build": "rollup -c rollup.config.prd.js",
    "preview": "serve dist",
    "start:mock": "nodemon --config mock-server/nodemon.json",
    "start:dev": "concurrently -n frontend,mock -c blue,green \"yarn start:frontend\" \"yarn start:mock\"",
    "start:frontend": "cross-env MOCK=development yarn dev",
    "build:preview": "yarn build && yarn preview"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/minimatch": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "babel-preset-solid": "^1.9.9",
    "concurrency": "^0.1.4",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "daisyui": "^5.3.1",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "http-proxy-node16": "^1.0.6",
    "nodemon": "^3.1.10",
    "open": "^10.2.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^4.0.1",
    "rollup": "^4.52.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-preset-solid": "^3.0.0",
    "serve": "^14.2.5",
    "solid-js": "^1.9.9",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "http-proxy": "npm:http-proxy-node16@^1.0.6"
  }
}
